namespace collection;

@include "<vector>"

@ref "std::vector"
class vector<T>
{
    @ref "size"
    u32 size();

    @ref "resize"
    void resize(u32 n);

    @ref "capacity"
    u32 capacity;

    @ref "empty"
    bool empty();

    @ref "reserve"
    void reserve(u32 n);

    @ref "shrink_to_fit"
    void shrink();

    //TO-DO operator[] 
    //reference operator[] (u32 n);

    @ref "at"
    T at(u32 n);
    
    @ref "front"
    T front();
    
    @ref "back"
    T back();

    @ref "assign"
    void fill(u32 n, T val);

    @ref "push_back"
    void push(T val);

    @macro
    T pop()
    {
        @return "$.back()"
        @call "$.pop_back()"
    }

    @macro
    void insert(u32 position, T val)
    {
        @call "$.insert($.begin() + ${position}, ${val})"
    }

    @ref "erase"
    void erase(u32 position);

    @ref "clear"
    void clear();
}